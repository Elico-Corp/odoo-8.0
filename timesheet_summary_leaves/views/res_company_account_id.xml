<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_company_account_id_view_form" model="ir.ui.view">
        <field name="name">res.company.view.form.inherit.res.company.account.id</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="account_id" string="Analytic Account Link to Leave Request"/>
            </xpath>
        </field>
    </record>

</odoo>

