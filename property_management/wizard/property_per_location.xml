<?xml version="1.0" encoding="utf-8"?>
<openerp>
     <data>

            <!--Wizard For Property Per Location-->
        <record id="property_per_location_wiz" model="ir.ui.view">
            <field name="name">Property Per Location Report</field>
            <field name="model">property.per.location</field>
            <field name="arch" type="xml">
              <form string="Property Per Location">
                <separator string = "Property Per Location Report" colspan="4"/>
                <group>
                    <field name="state_id" />
                </group>
                <footer>
                    <button special="cancel" string="Cancel" icon="gtk-cancel" class="oe_highlight"/>
                    <button name="print_report" string="Print" type="object" icon="gtk-print" class="oe_highlight"/>
                </footer>
            </form>
           </field>
        </record>

            <!--Action of Wizard For Property Per Location-->
        <act_window name="Property Per Location"
            res_model="property.per.location"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_property_per_location"/>
     </data>
</openerp>