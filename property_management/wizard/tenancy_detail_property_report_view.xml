<?xml version="1.0" encoding="utf-8"?>
<openerp>
     <data>
     	<record id="tenancy_property_report1" model="ir.ui.view">
            <field name="name">Tenancy by Property</field>
            <field name="model">tenancy.property.report</field>
            <field name="arch" type="xml">
              <form string="Tenancy report">
            	<separator string = "Tenancy report by Property" colspan="4" />
            	<group>
            	    <field name="property_id" />
	            	<field name="start_date" />
	            	<field name="end_date" />
            	</group>
            	<footer>
	            	<button special="cancel" string="Cancel" icon="gtk-cancel" class="oe_highlight"/>
	               	<button name="print_report" string="Print" type="object" icon="gtk-print" class="oe_highlight"/>
	               	<button name="open_tenancy_by_property_gantt" string="Gantt View" type="object" class="oe_highlight"/>
	               	<button name="open_tenancy_by_property_tree" string="Table View" type="object" class="oe_highlight"/>
            	</footer>
	       	</form>
           </field>
        </record>
        
        <act_window name="Tenancy by Property"
            res_model="tenancy.property.report"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_property_report_tenancy" />
     </data>
</openerp>