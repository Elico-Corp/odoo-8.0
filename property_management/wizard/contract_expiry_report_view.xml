<?xml version="1.0" encoding="utf-8"?>
<openerp>
     <data>
     	<record id="tenancy_property_report" model="ir.ui.view">
            <field name="name">Tenancy Expiry Report</field>
            <field name="model">contract.report</field>
            <field name="arch" type="xml">
              <form string="Tenancy report">
            	<separator string = "Tenancy Contract Expiry Report" colspan="4"/>
            	<group>
	            	<field name="start_date" />
	            	<field name="end_date" />
            	</group>
            	<footer>
            		<button special="cancel" string="Cancel" icon="gtk-cancel" class="oe_highlight"/>
	               	<button name="print_report" string="Print" icon="gtk-print" type="object" class="oe_highlight"/>
	               	<button name="open_contract_expiry_tree" string="Open" type="object" class="oe_highlight" />
            	</footer>
            	
	       	</form>
           </field>
        </record>
        
        <act_window name="Contract Expity"
            res_model="contract.report"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_contract_expiry" />
     </data>
</openerp>